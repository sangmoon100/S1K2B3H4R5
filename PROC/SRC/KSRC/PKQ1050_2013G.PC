             NVL(FUNDAMT3,0),   NVL(PGIVEAMT2,0),    NVL(HOUSEAMT2,0),    NVL(HOUSEINTAMT3,0),      /*HOUSEINTAMT3  주택자금(장기주택저당 차입 이자상환)   30년 이상 2010.01 KTH*/
             NVL(HOUSERENTAMT,0),NVL(HOUSERENTDED,0),NVL(HOUSVSUBAMT,0),  NVL(HOUSVEMPAMT,0), 
